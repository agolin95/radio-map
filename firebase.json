    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "radio-map",
          "region": "northamerica-northeast1"
        }
      }
    ]
  }
}